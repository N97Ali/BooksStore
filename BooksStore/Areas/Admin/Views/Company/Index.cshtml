 

<partial name="_Notification" />
<div class="Container">
    <div class="row pt-4 pd-3">
        <div class="col-6">
            <h2 class="text-primary ">
                Company List
            </h2>
        </div>
        <div class="text-end">
            <a asp-controller="Company" asp-action="Upsert" class="btn btn-primary">
                <i class="bi bi-plus-circle"></i> Create New Company
            </a>
        </div>
    </div>
    <table id="tblData" class="table table-bordered table-striped" width="100%">
        <thead>
            <tr>
                <th>
                    Name
                </th>
                <th>
                    Address
                </th>
                <th>
                    City
                </th>
                <th>
                    State
                </th>
                <th>
                    PhoneNumber
                </th>
            </tr>
        </thead>
       

    </table>
</div>


@section Scripts{

<script src="~/js/Company.js"></script>
}